{
  "name": "@flekschas/utils",
  "version": "0.33.0",
  "description": "A set of utility functions I use across projects",
  "author": {
    "name": "Fritz Lekschas",
    "url": "https://lekschas.de"
  },
  "keywords": [
    "utils"
  ],
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./animation": {
      "types": "./dist/animation.d.ts",
      "default": "./dist/animation.js"
    },
    "./array": {
      "types": "./dist/array.d.ts",
      "default": "./dist/array.js"
    },
    "./casing": {
      "types": "./dist/casing.d.ts",
      "default": "./dist/casing.js"
    },
    "./color": {
      "types": "./dist/color.d.ts",
      "default": "./dist/color.js"
    },
    "./conversion": {
      "types": "./dist/conversion.d.ts",
      "default": "./dist/conversion.js"
    },
    "./dom": {
      "types": "./dist/dom.d.ts",
      "default": "./dist/dom.js"
    },
    "./event": {
      "types": "./dist/event.d.ts",
      "default": "./dist/event.js"
    },
    "./format": {
      "types": "./dist/format.d.ts",
      "default": "./dist/format.js"
    },
    "./functional-programming": {
      "types": "./dist/functional-programming.d.ts",
      "default": "./dist/functional-programming.js"
    },
    "./geometry": {
      "types": "./dist/geometry.d.ts",
      "default": "./dist/geometry.js"
    },
    "./map": {
      "types": "./dist/map.d.ts",
      "default": "./dist/map.js"
    },
    "./math": {
      "types": "./dist/math.d.ts",
      "default": "./dist/math.js"
    },
    "./object": {
      "types": "./dist/object.d.ts",
      "default": "./dist/object.js"
    },
    "./other": {
      "types": "./dist/other.d.ts",
      "default": "./dist/other.js"
    },
    "./sorting": {
      "types": "./dist/sorting.d.ts",
      "default": "./dist/sorting.js"
    },
    "./string": {
      "types": "./dist/string.d.ts",
      "default": "./dist/string.js"
    },
    "./timing": {
      "types": "./dist/timing.d.ts",
      "default": "./dist/timing.js"
    },
    "./type-checking": {
      "types": "./dist/type-checking.d.ts",
      "default": "./dist/type-checking.js"
    },
    "./vector": {
      "types": "./dist/vector.d.ts",
      "default": "./dist/vector.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "npm run fix && rm -rf dist && tsc",
    "fix": "biome check --write .",
    "prepublishOnly": "npm run fix; npm run test; npm run build",
    "prerelease": "npm run build; zip -r dist.zip dist",
    "pretest": "npm run fix",
    "test": "vitest run --environment=happy-dom",
    "coverage": "vitest run --coverage --environment=happy-dom"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@vitest/coverage-v8": "^3.2.4",
    "d3-format": "^3.1.0",
    "fast-deep-equal": "^3.1.3",
    "gh-pages": "^6.3.0",
    "happy-dom": "^19.0.2",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "homepage": "https://github.com/flekschas/utils",
  "repository": {
    "type": "git",
    "url": "git://github.com/flekschas/utils.git"
  },
  "bugs": {
    "url": "https://github.com/flekschas/utils/issues"
  }
}
